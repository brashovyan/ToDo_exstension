{
    "manifest_version": 3,
    "name": "Shiva",
    "description": "Приложения для людей с сдвг. Либо просто список задач с напоминанием)",
    "version": "0.0.1",
    "key": "",
    "background": {
        "service_worker": "/background.js",
        "type": "module"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
          }
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "/content.js"
            ]
        }
    ],
    "options_page": "options.html",
    "devtools_page": "devtools.html",
    "permissions": [
        "storage", 
        "alarms", 
        "notifications",
        "identity",
        "identity.email"
    ],
    "oauth2": {
        "client_id": "407373553523-bpurq76lm602fque5eak7v87k4i3utqb.apps.googleusercontent.com",
        "scopes": [
            "profile email",
            "https://www.googleapis.com/auth/contacts",
            "https://www.googleapis.com/auth/calendar"
        ]
    },
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    }
}